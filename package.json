{
  "name": "url-shortener",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run db:generate:prod && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npm run db:generate:env",
    "db:generate:dev": "prisma generate --schema=prisma/schema.dev.prisma",
    "db:generate:prod": "prisma generate --schema=prisma/schema.prod.prisma",
    "db:generate:env": "npm run db:generate:${NODE_ENV:-dev}",
    "db:migrate:dev": "prisma migrate dev --schema=prisma/schema.dev.prisma",
    "db:migrate:prod": "prisma migrate deploy --schema=prisma/schema.prod.prisma",
    "db:push:dev": "prisma db push --schema=prisma/schema.dev.prisma",
    "db:push:prod": "prisma db push --schema=prisma/schema.prod.prisma",
    "vercel-build": "npm run db:generate:prod && npm run db:push:prod && next build",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@vercel/edge": "^1.2.2",
    "clsx": "^2.1.1",
    "nanoid": "^5.1.5",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.5.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "prisma": "^6.13.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "typescript": "^5"
  }
}
